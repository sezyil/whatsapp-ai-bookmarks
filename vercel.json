{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python@3.1.30",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "MAX_WORKERS": "1",
    "APP_MODULE": "app.main:app"
  },
  "functions": {
    "app/main.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
